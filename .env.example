# GCP Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1
GCP_ZONE=us-central1-a

# GCS Buckets
GCS_RAW_BUCKET=sec-raw-data-${GCP_PROJECT_ID}
GCS_PROCESSED_BUCKET=sec-processed-data-${GCP_PROJECT_ID}
GCS_ANALYTICS_BUCKET=sec-analytics-data-${GCP_PROJECT_ID}

# BigQuery Datasets
BQ_BRONZE_DATASET=bronze_sec
BQ_SILVER_DATASET=silver_sec
BQ_GOLD_DATASET=gold_sec

# SEC API Configuration
SEC_USER_AGENT=YourCompanyName contact@youremail.com
SEC_API_BASE_URL=https://data.sec.gov
SEC_BULK_DATA_URL=https://www.sec.gov/Archives/edgar/daily-index
SEC_RATE_LIMIT=10

# Dataproc Configuration
DATAPROC_CLUSTER_NAME=sec-processing-cluster
DATAPROC_REGION=us-central1
DATAPROC_SUBNET=default

# Airflow Configuration
AIRFLOW_DAG_SCHEDULE=0 2 * * *
AIRFLOW_EMAIL_ON_FAILURE=your-email@example.com

# Looker Configuration
LOOKER_INSTANCE_URL=https://your-instance.looker.com
LOOKER_CLIENT_ID=your-looker-client-id
LOOKER_CLIENT_SECRET=your-looker-client-secret

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
DATA_START_YEAR=2020
DATA_END_YEAR=2024

# Service Account Paths (for local development)
GOOGLE_APPLICATION_CREDENTIALS=./credentials/service-account-key.json
