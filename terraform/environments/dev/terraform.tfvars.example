# SEC EDGAR Analytics Platform - Development Environment
# TEMPLATE FILE - Copy to terraform.tfvars and fill in your values

# GCP Project Configuration
project_id = "your-gcp-project-id"
region     = "us-central1"
zone       = "us-central1-a"

# Billing Configuration
billing_account_id = "XXXXXX-XXXXXX-XXXXXX"
project_owners_email = "your-email@example.com"

# Budget Alert - Set to your monthly limit
budget_amount = 20

# Budget alert notification channels (optional)
budget_alert_notification_channels = []

# Data Scope - Adjust based on your needs and budget
data_start_year = 2023
data_end_year   = 2024

# SEC API Configuration
# REQUIRED: Must include your email for SEC compliance
sec_user_agent = "YourCompany your-email@example.com"

# Environment Labels
labels = {
  project     = "sec-edgar-analytics"
  managed_by  = "terraform"
  environment = "dev"
  owner       = "your-github-username"
  cost_center = "personal"
}
